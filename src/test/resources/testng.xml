<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="Automation Suite" verbose="1" parallel="tests" thread-count="2">

  <test name="Smoke Suite">
    <groups>
      <run>
        <include name="smoke"/>
      </run>
    </groups>
    <packages>
      <package name="tests.*"/>
    </packages>
  </test>

</suite>
